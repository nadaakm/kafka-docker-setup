{
    "name": "debezium_connecter_partner",
    "config": {
      "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
      "tasks.max": "1",
      "database.hostname": "<dbhost>",
      "database.port": "<porthost>",
      "database.user": "<dbuser>",
      "database.password": "<dbpswd>",
      "database.dbname": "<dbname>",
      "database.server.name": "<dbservername>",
      "table.include.list": "<schema.tablename>",
      "plugin.name": "pgoutput",
      "slot.name": "<slotname>",
      "publication.name": "dbz_publication",
      "database.history.kafka.bootstrap.servers": "kafka:9092",
      "database.history.kafka.topic": "schema-changes.inventory",
      "key.converter": "org.apache.kafka.connect.json.JsonConverter",
      "key.converter.schemas.enable": "false",
      "value.converter": "org.apache.kafka.connect.json.JsonConverter",
      "value.converter.schemas.enable": "false",
      "column.exclude.list": "schema.tablename.column1,schema.tablename.column2,schema.tablename.column3",
      "time.precision.mode": "connect",
      "snapshot.lock.timeout.ms": "6000000",
      "snapshot.mode": "initial",
      "transforms": "unwrap,castLatitude,castLongitude,timestampDate,timestampCreate,timestampWrite,timestampLast,timestampReconciliation,timestampCalendar,timestampDateRc,timestampSignup",
      "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
      "transforms.unwrap.drop.tombstones": "false",
      "decimal.handling.mode": "double",
      "transforms.castLatitude.type": "org.apache.kafka.connect.transforms.Cast$Value",
      "transforms.castLatitude.spec": "partner_latitude:FLOAT64",
      "transforms.castLongitude.type": "org.apache.kafka.connect.transforms.Cast$Value",
      "transforms.castLongitude.spec": "partner_longitude:FLOAT64",
      "transforms.timestampDate.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampDate.field": "date",
      "transforms.timestampDate.target.type": "Timestamp",
      "transforms.timestampDate.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampCreate.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampCreate.field": "create_date",
      "transforms.timestampCreate.target.type": "Timestamp",
      "transforms.timestampCreate.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampWrite.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampWrite.field": "write_date",
      "transforms.timestampWrite.target.type": "Timestamp",
      "transforms.timestampWrite.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampLast.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampLast.field": "message_last_post",
      "transforms.timestampLast.target.type": "Timestamp",
      "transforms.timestampLast.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampSignup.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampSignup.field": "signup_expiration",
      "transforms.timestampSignup.target.type": "Timestamp",
      "transforms.timestampSignup.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampReconciliation.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampReconciliation.field": "last_reconciliation_date",
      "transforms.timestampReconciliation.target.type": "Timestamp",
      "transforms.timestampReconciliation.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampCalendar.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampCalendar.field": "calendar_last_notif_ack",
      "transforms.timestampCalendar.target.type": "Timestamp",
      "transforms.timestampCalendar.format": "MM/dd/yyyy HH:mm:ss",
      "transforms.timestampDateRc.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
      "transforms.timestampDateRc.field": "x_date_rc",
      "transforms.timestampDateRc.target.type": "Timestamp",
      "transforms.timestampDateRc.format": "MM/dd/yyyy"
    }
  }
  
